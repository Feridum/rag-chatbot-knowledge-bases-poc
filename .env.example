# AWS Configuration
AWS_REGION=eu-central-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# S3 Vectors Configuration (for direct querying)
S3_VECTORS_BUCKET_ARN=arn:aws:s3vectors:region:account-id:vector-bucket/bucket-name
S3_VECTORS_INDEX_ARN=arn:aws:s3vectors:region:account-id:index/bucket-name/index-name
S3_BUCKET_NAME=kb-documents

# Embedding Model
EMBEDDING_MODEL=amazon.titan-embed-text-v2:0

# IAM Role (REQUIRED for setup script only)
# ARN of the IAM role with permissions to create a knowledge base
# This role needs permissions for: S3, S3 Vectors, and Bedrock
BEDROCK_KB_ROLE_ARN=arn:aws:iam::1234567890:role/KnowledgeBaseExecutionRole

# S3 Configuration
S3_BUCKET_NAME=kb-documents
S3_VECTORS_BUCKET_NAME=kb-vectors
S3_VECTORS_INDEX_NAME=kb-index

# Knowledge Base Configuration
KB_NAME=my-knowledge-base
EMBEDDING_MODEL=amazon.titan-embed-text-v2:0

# Note: ${TIMESTAMP} will be replaced automatically with Date.now()
# You can also provide specific names instead
